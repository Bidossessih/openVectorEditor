{
  "name": "open-vector-editor",
  "version": "7.1.2",
  "description": "Teselagen's Open Source Vector Editor",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3344",
    "localj5": "AMQP=localhost:5672 REMOTE_J5=http://localhost:10000 nwb serve-react-demo --port 3344",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "postpublish": "yarn deploy",
    "predeploy": "yarn buildWithDemo",
    "deploy": "gh-pages -d demo/dist",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "testcafe": "testcafe --inspect-brk chrome testcafe/",
    "l": "testcafe-live --dev chrome testcafe/",
    "c": "yarn cypress open",
    "test-e2e": "yarn cypress run --record --key 5d118c91-6b05-4e6f-899b-17783d834bf5",
    "rt": "yarn cypress run --spec cypress/integration/tabs.js",
    "test": "jest --env=jsdom --maxWorkers 2",
    "test-coverage": "yarn test -- --coverage",
    "test:staged": "cross-env CI=true yarn test -- --findRelatedTests",
    "test-fix": "git reset . && cross-env CI=true yarn test --onlyChanged --updateSnapshot",
    "test-watch": "yarn test --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "color": "^3.1.0",
    "combokeys": "^3.0.0",
    "deep-equal": "^1.0.1",
    "file-saver": "^1.3.8",
    "jest-puppeteer": "^3.5.1",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.11",
    "lru-memoize": "^1.0.2",
    "node-interval-tree": "^1.3.3",
    "paths-js": "^0.4.5",
    "popper.js": "^1.14.5",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.10.0",
    "randomcolor": "^0.5.3",
    "react-beautiful-dnd": "^10.0.1",
    "react-draggable": "^3.0.3",
    "react-dropzone": "^4.2.11",
    "react-easy-state": "^5.0.1",
    "react-list": "^0.8.10",
    "react-measure": "^2.1.3",
    "react-sizeme": "^2.5.2",
    "react-to-print": "^2.0.0-alpha-4",
    "recompose": "^0.27.1",
    "redux": "^4.0.1",
    "redux-act": "^1.7.4",
    "redux-ignore": "^1.2.5",
    "reselect": "^3.0.1",
    "tg-react-reflex": "^2.0.10",
    "typeface-ubuntu-mono": "^0.0.54",
    "uniqid": "^4.1.1"
  },
  "peerDependencies": {
    "apollo-cache-invalidation": "^0.0.3",
    "bio-parsers": "^4.2.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.6.0",
    "teselagen-react-components": "^24.0.1",
    "tg-client-query-builder": "^2.12.1",
    "ve-range-utils": "^2.3.0",
    "ve-sequence-utils": "^3.2.11"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.9.0",
    "@blueprintjs/datetime": "^3.4.0",
    "apollo-cache-invalidation": "^0.0.3",
    "apollo-client": "2.4.6",
    "axios": "^0.18.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bio-parsers": "5.0.1",
    "chance": "^1.0.16",
    "cross-env": "^5.1.6",
    "cypress": "^3.1.2",
    "drag-mock": "^1.4.0",
    "enzyme": "^3.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-teselagen": "^4.0.12",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-cypress": "^2.1.2",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "gh-pages": "^1.2.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.0",
    "husky": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "install-peerdeps": "1.9.0",
    "jest": "^23.6.0",
    "lint-staged": "8.0.5",
    "moment": "2.22.2",
    "nodemon": "1.18.6",
    "nwb": "0.23.0",
    "prettier": "1.15.2",
    "react": "16.6.3",
    "react-addons-perf": "^15.4.2",
    "react-apollo": "^2.2.4",
    "react-collapsible": "^2.2.0",
    "react-dom": "16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.3.1",
    "react-select": "^1.0.0-rc.10",
    "react-table": "^6.8.6",
    "react-transition-group": "^2.3.1",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "teselagen-react-components": "24.2.96",
    "testcafe": "^0.23.2",
    "testcafe-live": "^0.1.4",
    "tg-client-query-builder": "^2.12.2",
    "ve-range-utils": "^2.5.1",
    "ve-sequence-utils": "3.3.6",
    "why-did-you-update": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/openVectorEditor",
  "keywords": [
    "react-component",
    "plasmid",
    "vector",
    "editor",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
